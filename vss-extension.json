{
    "manifestVersion": 1,
    "id": "contributions",
    "version": "1.6.87",
    "name": "Contributions Graph",
    "scopes": [
        "vso.work",
        "vso.work_write",
        "vso.code"
    ],
    "description": "View account contributions by user.",
    "publisher": "ottostreifel",
    "icons": {
        "default": "img/logo.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services.Cloud"
        },
        {
            "id": "Microsoft.TeamFoundation.Server",
            "version": "[15.0,)"
        }
    ],
    "tags": [
        "Work Items",
        "Work Item",
        "Git",
        "Pull Request",
        "Commit",
        "Visualization",
        "Contributions"
    ],
    "content": {
        "details": {
            "path": "details.md"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/ostreifel/vsts-contributions"
    },
    "branding": {
        "color": "rgb(220, 235, 252)",
        "theme": "light"
    },
    "files": [
        {
            "path": "dist",
            "addressable": true
        },
        {
            "path": "img",
            "addressable": true
        },
        {
            "path": "html",
            "addressable": true
        }
    ],
    "categories": [
        "Collaborate"
    ],
    "contributions": [
        {
          "id": "moved-hubs-banner",
          "type": "ms.vss-web.global-message-banner",
          "targets": [
            "ms.vss-web.home-hub-group"
          ],
          "properties": {
            "message": "i18n:The Contributions Graph pivot has moved from the dashboards hub to the work hub.",
            "level": "info"
          }
        },
        {
            "id": "contributions-hub",
            "type": "ms.vss-web.hub",
            "description": "View code and work item contributions.",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "group": "contributed",
                "name": "Contributions Graph",
                "uri": "html/contributionsHub.html"
            }
        }
    ]
}